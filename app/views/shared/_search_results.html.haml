%section.row{'ng-if' => 'data.notes'}
  %h1.text-center.h2= 'Results'
  %ul.list-unstyled
    %li{'ng-repeat' => 'note in data.notes'}
      %section.well.col-md-6.col-md-offset-3
        %h1.h3.col-md-6.text-right{'style' => 'margin-top: 0'}
          %a{'href' => '', "ng-click" => "view(note.id)"} {{ note.kanji }}
        .col-md-6
          %button.btn.btn-info{'ng-click' => "edit(note.id)"}= 'Edit'
          %button.btn.btn-danger{'ng-click' => "delete(note.id)"}= 'Delete'
